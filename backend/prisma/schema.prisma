datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Model {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  data      Data[]
}

model Data {
  id        Int      @id @default(autoincrement())
  modelId   Int
  model     Model    @relation(fields: [modelId], references: [id])
  // Add other fields based on your CSV structure
  // For example:
  date      DateTime
  value     Float
}